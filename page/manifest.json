{
    "name": "File Handling Demo",
    "start_url": "/",
    "display": "standalone",
    "file_handlers": [
      {
        "action": "/page/",
        "accept": {
          "text/plain": [".webloc"],
          "application/octet-stream": [".webloc_2_"]
        }
      }
    ],
   
    "icons": [
        {
            "src": "ios/192.png",
            "sizes": "192x192"
        },
        {
            "src": "ios/512.png",
            "sizes": "512x512"
        }
    ]
}
  